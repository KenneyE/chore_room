<h1>App Academy Residents</h1>

<ul id="residents">
  <% @users.each do |user| %>
    
    <li>
      <a href="<%= user_url(user.id) %>">
        <%= image_tag(user.photo) %>
        <%= user.name %>
      </a>
      <% if user.admin %>
        (Admin)
      <% end %>
    </li>
    
  <% end %>
</ul>